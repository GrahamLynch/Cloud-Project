<h1>Other Peoples Profiles</h1>
<div class="row">
<% Profile.all.each do |user| %>
<div class ="col-lg-4">
<div class="card" style="width: 18rem;">
  <div class="card-body">
    <img src="https://image.flaticon.com/icons/svg/1864/1864593.svg" class="card-img-top" alt="...">
    <h5 class="card-title"><%= user.name %></h5>
    <a href="/users/<%= user.user.id %>/profiles/1" class="btn btn-primary">View Profile</a>
  </div>
</div>
</div>
<% end %>
</div>
